{
  "ConnectionStrings": {
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Debug"
    }
  },
  "Tracing": {
    "appName": "Fs.Blazor.Service"
  },
  "Hosting": {
    "appKey": "Fs.Blazor.Service",
    "httpsPort": "5001",
    "useUrls": "https://fs-blazor-service.netpoc.com:5001",
    "urlPrefixes": "https://127.0.0.1:5001"
  },
  "OidcProviders": {
    "oidc": {
      "ClientID": "Fs.Blazor.Service",
      "ClientSecret": "secret",
      "ResponseType": "code",
      "SaveTokens": true,
      "HttpsMetadata": false,
      "ClaimsUserEndpoint": true,
      "Scope": "WebAPI",
      "Events": true
    }
  },
  "AllowedHosts": "*"
}
